{
  "name": "learn_gulp",
  "private": true,
  "scripts": {
    "gulp": "gulp --gulpfile ./gulp/index.js --cwd ./",
    "default": "npm run gulp -- default",
    "build": "cross-env NODE_ENV=production npm run default",
    "dev": "cross-env NODE_ENV=development npm run default",
    "nodemon": "nodemon --config ./nodemon.json",
    "nodemon:dev": "npm run nodemon -- dev",
    "nodemon:gulp": "npm run nodemon -- gulp",
    "nodemon:prod": "npm run nodemon -- build",
    "start": "npm run nodemon:dev"
  },
  "browserslist": [
    "last 5 versions",
    "ie >= 11"
  ],
  "dependencies": {
    "autoprefixer": "9.7.4",
    "cross-env": "7.0.0",
    "cssnano": "4.1.10",
    "del": "5.1.0",
    "gulp": "4.0.2",
    "gulp-cached": "1.1.1",
    "gulp-changed": "4.0.2",
    "gulp-concat": "2.6.1",
    "gulp-debug": "4.0.0",
    "gulp-if": "3.0.0",
    "gulp-postcss": "8.0.0",
    "gulp-rename": "2.0.0",
    "gulp-stylus": "2.7.0"
  },
  "devDependencies": {
    "browser-sync": "2.26.7",
    "gulp-sourcemaps": "2.6.5",
    "nodemon": "2.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yialo/learn_gulp.git"
  }
}
